{
    "name": "slince/shopify-api-php",
    "description": "Shopify API Client for PHP",
    "keywords": ["shopify", "rest", "shopify-sdk", "shopify-api", "shopify-client", "shopify-client-php"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Tao",
            "email": "taosikai@yeah.net"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzlehttp/guzzle": "^6.0",
        "doctrine/inflector": "^1.0",
        "slince/di": "^2.0",
        "nesbot/carbon": "^1.22",
        "symfony/serializer": "^2.7|^3.0",
        "symfony/property-access": "^2.7|^3.0",
        "jms/serializer": "^1.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Slince\\Shopify\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slince\\Shopify\\Tests\\": "./tests"
        }
    }
}
